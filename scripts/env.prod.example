# ===========================================
# ПЕРЕМЕННЫЕ ОКРУЖЕНИЯ ДЛЯ ПРОДАКШН ПРОФИЛЯ
# ===========================================
# Скопируйте этот файл в .env.prod и заполните реальными значениями

# Tinkoff API - продакшн токен (ОБЯЗАТЕЛЬНО!)
# Получите токен в личном кабинете Tinkoff Invest: https://www.tinkoff.ru/invest/settings/api/
T_INVEST_PROD_TOKEN=your_production_token_here

# База данных - продакшн (облачная PostgreSQL)
# Замените на ваши реальные данные подключения
SPRING_DATASOURCE_PROD_URL=jdbc:postgresql://your-prod-host:5432/your-database
SPRING_DATASOURCE_PROD_USERNAME=your_prod_username
SPRING_DATASOURCE_PROD_PASSWORD=your_prod_password

# Spring профиль
SPRING_PROFILES_ACTIVE=prod

# Дополнительные настройки для продакшна
APP_PROD_MODE=true
APP_PROD_DATA_BACKUP_ENABLED=true


