# ===========================================
# ПЕРЕМЕННЫЕ ОКРУЖЕНИЯ ДЛЯ ТЕСТОВОГО ПРОФИЛЯ
# ===========================================
# Скопируйте этот файл в .env.test и заполните реальными значениями

# Tinkoff API - тестовый токен
T_INVEST_TEST_TOKEN=token

# База данных - тестовая (локальная PostgreSQL)
SPRING_DATASOURCE_TEST_URL=jdbc:postgresql://localhost:5434/postgres
SPRING_DATASOURCE_TEST_USERNAME=postgres
SPRING_DATASOURCE_TEST_PASSWORD=password

# Spring профиль
SPRING_PROFILES_ACTIVE=test

# Дополнительные настройки для тестов
APP_TEST_MODE=true
APP_TEST_DATA_CLEANUP=true
